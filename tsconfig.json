{
  "compilerOptions": {
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "target": "es2015",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "Web/dist",
    "baseUrl": "."
  },
  "include": [
    "Web/**/*.ts"
  ],
  "exclude": [
    "Web/dist"
  ]
}
