{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "target": "es2015",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "Web/dist",
  },
  "include": [
    "Web/**/*.ts"
  ],
  "exclude": [
    "Web/dist",
    "Web/dist-cjs"
  ]
}
